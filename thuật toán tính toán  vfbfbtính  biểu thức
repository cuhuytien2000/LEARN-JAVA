package sgsdgdfd;

import java.util.Scanner;


public class luentapswich {
	
	public static void hình1(int n) {
		for(int i = 0; i<n; i++)
		{
			for(int j =i; j<n; j++)
			{
				System.out.print("*");
			}
			System.out.println();
		}
	}

	public static void main(String[] args) {
		hình1(10);
	}
}











