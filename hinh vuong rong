# LEARN-JAVApackage fsfffd;

public class bdfbfbfr {
	public static void hinh1(int n) {
		for(int i =0;i<=n;i++) {
			for(int j =0; j<=n;j++)
			{
				if(((j <=n)&&(i==0 ||i==n))||((i <=n)&&(j==0 ||j==n)))
				
					System.out.print("* ");
				else 
					System.out.print("  ");
			}
			System.out.println("");
		}
	}
	public static void main(String[] args) {
		hinh1(3);
	}

}
